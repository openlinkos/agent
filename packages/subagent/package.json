  },
  "homepage": "https://openlinkos.com",
  "bugs": {
    "url": "https://github.com/openlinkos/agent/issues"
  },
  "keywords": [
    "openlinkos",
    "subagent",
    "agent",
    "delegation",
    "composition"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
