{
  "name": "@openlinkos/channel-slack",
  "version": "0.1.0",
  "description": "OpenLinkOS Channel â€” Slack app adapter for deploying agents to Slack",
  "license": "MIT",
  "homepage": "https://openlinkos.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/openlinkos/agent.git",
    "directory": "channels/channel-slack"
  },
  "bugs": {
    "url": "https://github.com/openlinkos/agent/issues"
  },
  "keywords": ["openlinkos", "slack", "bot", "channel", "agent"],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@openlinkos/channel": "workspace:*"
  }
}
